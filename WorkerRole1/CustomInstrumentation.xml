<?xml version="1.0" encoding="utf-8" ?>
<extension xmlns="urn:newrelic-extension">
  <instrumentation>
    
    <tracerFactory name="NewRelic.Agent.Core.Tracer.Factories.BackgroundThreadTracerFactory" metricName="Worker/OnStart">
      <match assemblyName="WorkerRole1" className="WorkerRole1.WorkerRole">
        <exactMethodMatcher methodName="OnStart" />
      </match>
    </tracerFactory>
    <tracerFactory name="NewRelic.Agent.Core.Tracer.Factories.BackgroundThreadTracerFactory" metricName="Worker/OnStop">
      <match assemblyName="WorkerRole1" className="WorkerRole1.WorkerRole">
        <exactMethodMatcher methodName="OnStop" />
      </match>
    </tracerFactory>
    <tracerFactory name="NewRelic.Agent.Core.Tracer.Factories.BackgroundThreadTracerFactory" metricName="Worker/Run">
      <match assemblyName="WorkerRole1" className="WorkerRole1.WorkerRole">
        <exactMethodMatcher methodName="Run" />
      </match>
    </tracerFactory>

    <tracerFactory name="NewRelic.Agent.Core.Tracer.Factories.BackgroundThreadTracerFactory" metricName="EventProcessor/CloseAsync">
      <match assemblyName="WorkerRole1" className="WorkerRole1.EventProcessor">
        <exactMethodMatcher methodName="CloseAsync" />
      </match>
    </tracerFactory>
    <tracerFactory name="NewRelic.Agent.Core.Tracer.Factories.BackgroundThreadTracerFactory" metricName="EventProcessor/OpenAsync">
      <match assemblyName="WorkerRole1" className="WorkerRole1.EventProcessor">
        <exactMethodMatcher methodName="OpenAsync" />
      </match>
    </tracerFactory>
    <tracerFactory name="NewRelic.Agent.Core.Tracer.Factories.BackgroundThreadTracerFactory" metricName="EventProcessor/ProcessEventsAsync">
      <match assemblyName="WorkerRole1" className="WorkerRole1.EventProcessor">
        <exactMethodMatcher methodName="ProcessEventsAsync" />
      </match>
    </tracerFactory>

    <tracerFactory name="NewRelic.Agent.Core.Tracer.Factories.BackgroundThreadTracerFactory" metricName="RemoteProcessServer/Setup">
      <match assemblyName="WorkerRole1" className="SuperSocket.QuickStart.RemoteProcessService.RemoteProcessServer">
        <exactMethodMatcher methodName="Setup" />
      </match>
    </tracerFactory>
    <tracerFactory name="NewRelic.Agent.Core.Tracer.Factories.BackgroundThreadTracerFactory" metricName="RemoteProcessServer/Stop">
      <match assemblyName="WorkerRole1" className="SuperSocket.QuickStart.RemoteProcessService.RemoteProcessServer">
        <exactMethodMatcher methodName="Stop" />
      </match>
    </tracerFactory>

    <tracerFactory name="NewRelic.Agent.Core.Tracer.Factories.BackgroundThreadTracerFactory" metricName="RemoteProcessSession/HandleException">
      <match assemblyName="WorkerRole1" className="SuperSocket.QuickStart.RemoteProcessService.RemoteProcessSession">
        <exactMethodMatcher methodName="HandleException" />
      </match>
    </tracerFactory>
    <tracerFactory name="NewRelic.Agent.Core.Tracer.Factories.BackgroundThreadTracerFactory" metricName="RemoteProcessSession/OnSessionClosed">
      <match assemblyName="WorkerRole1" className="SuperSocket.QuickStart.RemoteProcessService.RemoteProcessSession">
        <exactMethodMatcher methodName="OnSessionClosed" />
      </match>
    </tracerFactory>
    <tracerFactory name="NewRelic.Agent.Core.Tracer.Factories.BackgroundThreadTracerFactory" metricName="RemoteProcessSession/OnSessionStarted">
      <match assemblyName="WorkerRole1" className="SuperSocket.QuickStart.RemoteProcessService.RemoteProcessSession">
        <exactMethodMatcher methodName="OnSessionStarted" />
      </match>
    </tracerFactory>
    
  </instrumentation>
</extension>
