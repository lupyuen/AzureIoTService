<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <section name="superSocket" type="SuperSocket.SocketEngine.Configuration.SocketServiceConfig, SuperSocket.SocketEngine" />
  </configSections>
  <superSocket>
    <servers>
      <server name="RemoteProcessServer" serverTypeName="remoteProcess" ip="Any" port="2012" />
    </servers>
    <serverTypes>
      <add name="remoteProcess" type="SuperSocket.QuickStart.RemoteProcessService.RemoteProcessServer, WorkerRole1" />
    </serverTypes>
  </superSocket>
  <system.diagnostics>
    <trace>
      <listeners>
        <add type="Microsoft.WindowsAzure.Diagnostics.DiagnosticMonitorTraceListener, Microsoft.WindowsAzure.Diagnostics, Version=2.7.0.0, Culture=neutral, PublicKeyToken=31bf3856ad364e35" name="AzureDiagnostics">
        </add>
      </listeners>
    </trace>
  </system.diagnostics>
  <appSettings>
    <add key="NewRelic.AppName" value="IoTWorker" />
  </appSettings>
</configuration>